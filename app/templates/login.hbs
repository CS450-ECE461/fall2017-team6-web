<div align="center">
<h1>Logo</h1>
  <div class="Login-info">
    <div class="form-group">
      {{!--<label>Username</label>--}}
      {{input type="text" class="form-control" value=username
      enter="login" placeholder="User"}}
    </div>
    <div class="form-group">
      {{!--<label>Password</label>--}}
      {{input type="text" class="form-control" value=password
      enter="login" placeholder="Password"}}
    </div>
    {{!#link-to 'dashboard' tagName='button'}}{{!Sign In}}{{!/link-to}}
    <button {{action "login"}}>Sign In</button>

  </div>
  <div class="container">
    <p> Don't have an account yet?
      {{#link-to 'signup' class="button"}}
        Sign Up.
      {{/link-to}}
    </p>
    </div>
</div>
